{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true,
    },
    "rules": {
        "indent": ["error", 4],
        "no-trailing-spaces": ["error", "skipBlankLines": true],
        "no-console": 1,
        "padded-blocks": 0,
        "no-restricted-syntax": 0,
        "max-len": 0
    },
}
